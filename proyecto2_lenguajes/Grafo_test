	A [label=i shape=circle]
	B [label=p shape=circle]
	A -> B [label="λ,λ;#" constraint=false]
	C [label=q shape=circle]
	B -> C [label="λ,λ;S" constraint=false]
	C -> C [label="λ, S; zMNz
